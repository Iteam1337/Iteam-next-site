{
  "extends": [
    "eslint-config-next",
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "groups": [
          ["external", "builtin"],
          "internal",
          ["index", "sibling", "parent"]
        ]
      }
    ]
  }
}
